#!/bin/bash

for i in ART_USER_NAME ART_USER_EMAIL \
ART_ROOT ART_REPO_ROOT ARTCLI_HOME \
ARTCLI_CONST_VERSION ARTCLI_CONST_EXEC ARTCLI_CONST_DIR \
ARTCLI_CONST_ABOUT_FILE ARTCLI_CONST_BEFORE_FILE \
ARTCLI_PROJECT ARTCLI_PROJECT_ROOT ARTCLI_PROJECT_PATH ARTCLI_ACTION \
ARTCLI_ACTION_DIR_PATH ARTCLI_ACTION_FILE_NAME ARTCLI_CALLER
do 
  echo "$i = ${!i}" 
done
